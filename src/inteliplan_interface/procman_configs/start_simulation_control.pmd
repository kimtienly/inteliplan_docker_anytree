group "ANYmal" {
    cmd "sim" {
        exec = "roslaunch inteliplan_interface simulation.launch";
        host = "localhost";
    }
}

group "Z1" {
    cmd "sim_ctrl" {
        exec = "/bin/sh -c 'rosparam set /z1/dt 0.0025; roslaunch anytree_control standalone_arm_control.launch'";
        host = "localhost";
    }
}