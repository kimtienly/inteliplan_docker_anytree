<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="x" default="-0.4" />
  <arg name="y" default="0" />
  <arg name="z" default="0.7" />
  <arg name="model_name" default="virtual_camera" />
  <arg name="model_file" default="$(find inteliplan_interface)/configs/models/virtual_camera/model.sdf" />

  <node pkg="gazebo_ros" type="spawn_model" name="spawn_virtual_camera" output="screen"
        args="-file $(arg model_file) -sdf -model $(arg model_name) -x $(arg x) -y $(arg y) -z $(arg z)" />
</launch>
